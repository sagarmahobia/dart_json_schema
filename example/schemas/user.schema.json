{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "User",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "description": "Unique identifier for the user",
      "examples": [
        1,
        2,
        3
      ]
    },
    "name": {
      "type": "string",
      "description": "The user's full name",
      "examples": [
        "John Doe",
        "Jane Smith"
      ]
    },
    "email": {
      "type": "string",
      "description": "User's email address",
      "examples": [
        "john@example.com",
        "jane@example.com"
      ]
    },
    "age": {
      "type": "integer",
      "description": "User's age in years",
      "examples": [
        25,
        30,
        45
      ]
    },
    "isActive": {
      "type": "boolean",
      "description": "Whether the user account is active",
      "examples": [
        true,
        false
      ]
    },
    "registrationDate": {
      "type": "string",
      "description": "Date when the user registered",
      "examples": [
        "2023-01-15T10:00:00Z",
        "2023-03-20T14:30:00Z"
      ]
    }
  },
  "required": [
    "id",
    "name",
    "email",
    "age",
    "isActive",
    "registrationDate"
  ],
  "$defs": {}
}
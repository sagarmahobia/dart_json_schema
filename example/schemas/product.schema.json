{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Product",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "description": "Unique identifier for the product",
      "examples": [
        101,
        102
      ]
    },
    "name": {
      "type": "string",
      "description": "Name of the product",
      "examples": [
        "Laptop",
        "Mouse"
      ]
    },
    "price": {
      "type": "number",
      "description": "Price of the product",
      "examples": [
        1200.5,
        25.99
      ]
    },
    "inStock": {
      "type": "boolean",
      "description": "Availability status of the product",
      "examples": [
        true,
        false
      ]
    },
    "tags": {
      "$ref": "#/$defs/List<String>",
      "description": "List of tags associated with the product",
      "examples": [
        [
          "electronics",
          "computers"
        ],
        [
          "accessories"
        ]
      ]
    },
    "manufacturer": {
      "$ref": "#/$defs/Map<String, dynamic>",
      "description": "Details about the product manufacturer",
      "examples": [
        "{'name' : 'TechCorp', 'country' : 'USA'}",
        "{'name' : 'GlobalGadgets', 'country' : 'China'}"
      ]
    },
    "category": {
      "type": "string",
      "description": "Category of the product",
      "examples": [
        "Electronics",
        "Office",
        "Home"
      ]
    },
    "listedAt": {
      "type": "string",
      "description": "Date and time when the product was listed",
      "examples": [
        "2023-01-15T10:00:00Z",
        "2023-03-20T14:30:00Z"
      ]
    }
  },
  "required": [
    "id",
    "name",
    "price",
    "inStock",
    "tags",
    "manufacturer",
    "category",
    "listedAt"
  ],
  "$defs": {
    "List<String>": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "Map<String, dynamic>": {
      "type": "object",
      "properties": {},
      "required": []
    }
  }
}